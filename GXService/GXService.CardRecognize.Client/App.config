<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <netTcpBinding>
                <binding name="NetTcpBinding_ICardsRecognizer">
                    <security mode="Message">
                        <message clientCredentialType="UserName" />
                    </security>
                </binding>
                <binding name="NetTcpBinding_ICardTypeParser">
                    <security mode="Message">
                        <message clientCredentialType="UserName" />
                    </security>
                </binding>
                <binding name="NetTcpBinding_IBroadcast">
                    <security mode="Message">
                        <message clientCredentialType="UserName" />
                    </security>
                </binding>
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://localhost/GXService/CardRecognize/CardRecognizeService.svc"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_ICardsRecognizer"
                contract="CardRecognize.ICardsRecognizer" name="NetTcpBinding_ICardsRecognizer">
                <identity>
                    <certificate encodedValue="" />
                </identity>
            </endpoint>
            <endpoint address="net.tcp://localhost/GXService/CardRecognize/CardTypeParseService.svc"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_ICardTypeParser"
                contract="CardTypeParse.ICardTypeParser" name="NetTcpBinding_ICardTypeParser">
                <identity>
                    <certificate encodedValue="" />
                </identity>
            </endpoint>
            <endpoint address="net.tcp://localhost/GXService/Broadcast/BroadcastService.svc"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IBroadcast"
                contract="Broadcast.IBroadcast" name="NetTcpBinding_IBroadcast">
                <identity>
                    <certificate encodedValue="" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>